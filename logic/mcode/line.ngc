(filename line.ngc)

o<line> sub
#<DIAM> = #1
(Ширина насения от.. до... шаг...)
#<K_> =  #2
(Точка начала насения X0 Y0 )
#<X0> =  #3
#<Y0> =  #4
(Длина  нанесения  )
#<X_test> =  #5
(Перемещние по оси Z)
#<Zwork> =  #6
#<Zsave> =  #7
(Скорость нанесения)
#<FEED> =  #8
(Обороты миксера)
#<SPEED> =  #9

   G54 G17 G40 G49 G90 G92.1 G94 G64 P0.001
    ;M400J[#<DIAM> ]K[#<K_>]F[#<FEED>]S[#<SPEED>] ;установить параметры нанесения
    M115 P[#<K_>]Q[#<FEED>]
    (PRINT,>> O101:  #<K_>)
